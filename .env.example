# Transport: "stdio" (default, for Claude Desktop) or "http" (for remote access)
THINGS_MCP_TRANSPORT=stdio

# HTTP transport settings (only used when THINGS_MCP_TRANSPORT=http)
THINGS_MCP_HOST=127.0.0.1
THINGS_MCP_PORT=8765

# API key for HTTP transport authentication (auto-generated on first run if empty)
THINGS_MCP_API_KEY=

# Things URL scheme auth token (for URL scheme fallback writes; auto-detected from Things if empty)
THINGS_AUTH_TOKEN=
